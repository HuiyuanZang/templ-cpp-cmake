{
    // For more information, visit: https://code.visualstudio.com/docs/cpp/c-cpp-properties-schema-reference
    "env": {
        "myDefaultIncludePath": [
            "${workspaceFolder}",
            "${workspaceFolder}/project",
            "${workspaceFolder}/project/include",
            "${workspaceFolder}/project/include/external",
            "${workspaceFolder}/project/include/interface",
            "${workspaceFolder}/project/include/internal"
        ],
        // "compilerPath": "C:\\msys64\\mingw64\\bin\\g++.exe"
        "compilerPath": "/usr/bin/g++"
    },
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/project/include/external/**",
                "${workspaceFolder}/project/include/internal/**",
                "${workspaceFolder}/project/include/interface/**"
            ],
            "defines": [],
            "compilerPath": "/usr/bin/g++",
            "cStandard": "c11",
            "cppStandard": "c++14",
            "browse": {
                "path": [
                    "${workspaceFolder}/project/include/external/**",
                    "${workspaceFolder}/project/include/internal/**",
                    "${workspaceFolder}/project/include/interface/**"
                ],
                "limitSymbolsToIncludedHeaders": false,
                "databaseFilename": ""
            },
            "intelliSenseMode": "${default}",
            "configurationProvider": "ms-vscode.cmake-tools"
        },
        {
            "name": "Win32",
            "includePath": [
                "${workspaceFolder}/project/include/external/**",
